package test;

public class Test { 
	  class A { } 
	  static class B { }
	  public static void main(String[] args) { 
	    /*will fail - compilation error, you need an instance of Test to instantiate A*/
	    A a = a.new A(); 
	    /*will compile successfully, not instance of Test is needed to instantiate B */
	    B b = new B(); 
	  }
	}