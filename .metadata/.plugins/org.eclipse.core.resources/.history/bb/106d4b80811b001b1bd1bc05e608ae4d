
public class Outer {

	void outerMethod()
	{
		System.out.println("this is inside outer Method");
		
		class Inner //this is my method local inner class
		{
			void innerMethod()
			{
				System.out.println("inside Inner method");
			}
			
		}
		
		Inner y = new Inner();
		
	}
